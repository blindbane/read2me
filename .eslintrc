{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true, 
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0
  },
  "globals": {
    "DEBUG": false
  }
}